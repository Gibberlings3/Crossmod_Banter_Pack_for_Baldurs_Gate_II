BACKUP ~weidu_external\crossmodbg2\backup~
//AUTHOR ~Grim Squeaker, K'aeloree, and Berelinde, maintained at www.gibberlings3.net~
SUPPORT ~https://www.gibberlings3.net/forums/forum/55-crossmod-banter-pack/~

/* backwards compatibility is a PITA, but necessary */
NO_IF_EVAL_BUG

VERSION ~v25~ //put version number here, not inside setup.tra
AUTO_TRA ~crossmodbg2/tra/%s~
README ~crossmodbg2/readme-crossmodbg2.html~


ALWAYS

  ACTION_IF NOT VARIABLE_IS_SET bg2_chapter THEN BEGIN // check to make this happen only once per install
  
    ACTION_IF GAME_IS ~eet~ BEGIN
      OUTER_SET bg2_chapter = 12
    END ELSE BEGIN
      OUTER_SET bg2_chapter = 0
    END
    OUTER_FOR (i=1; i<=10; i=i+1) BEGIN
      OUTER_SET bg2_chapter = bg2_chapter + 1
      OUTER_SPRINT name_source ~bg2_chapter_%i%~
      OUTER_SET EVAL ~%name_source%~ = bg2_chapter
    END
  
    ACTION_IF GAME_IS ~bgee tutu bg1 totsc tutu_totsc~ THEN BEGIN
      FAIL @1 /* WARNING. THIS MOD IS FOR BG2 OR BGT. DO NOT INSTALL THIS MOD. */
    END

   INCLUDE ~crossmodbg2/lib/extra_regexp_vars.tph~
   OUTER_SPRINT newline ~%WNL%%LNL%%MNL%%TAB% ~

/* German version - as long as it's not fully translated, copy files around so the "german" folder is complete with all German translations */
    ACTION_IF ("%LANGUAGE%" STRING_COMPARE_CASE "german" = 0) THEN BEGIN
      COPY ~crossmodbg2/tra/english~  ~crossmodbg2/tra/%LANGUAGE%~
      COPY ~crossmodbg2/tra/incomplete/%LANGUAGE%~  ~crossmodbg2/tra/%LANGUAGE%~
    END

    ACTION_IF GAME_IS ~bg2ee eet~ THEN BEGIN
      PRINT @2 /* BG2:EE detected... */
      ACTION_DEFINE_ARRAY cdnoconvert BEGIN soa_khalid-skiecostonesoul END // both EE mods, so already in utf-8
	  ACTION_DEFINE_ARRAY cdreload BEGIN setup END
      LAF HANDLE_CHARSETS INT_VAR infer_charsets = 1 STR_VAR tra_path = ~crossmodbg2/tra~ noconvert_array = cdnoconvert END
    END
    ACTION_IF GAME_IS ~soa~ THEN BEGIN
      PRINT @3 /* BGII:SoA detected... */
    END
    ACTION_IF GAME_IS ~tob~ THEN BEGIN
      PRINT @4 /* BGII:ToB detected... */
    END
    ACTION_IF GAME_IS ~bgt~ THEN BEGIN
      PRINT @5 /* BGT detected... */
    END

<<<<<<<< .../crossmodbg2-inlined/blank.log
>>>>>>>>
	
  END    
  
END

AUTO_TRA ~crossmodbg2/tra/%s~
LANGUAGE ~English~ ~english~ ~crossmodbg2/tra/english/setup.tra~ 
LANGUAGE ~Deutsch (Teildeutsche Version, der Rest ist in Englisch)~ ~german~ ~crossmodbg2/tra/english/setup.tra~ // reads the English setup.tra for now!
LANGUAGE ~Russian (incomplete translation)~ ~russian~ ~crossmodbg2/tra/russian/setup.tra~ 
		
			
